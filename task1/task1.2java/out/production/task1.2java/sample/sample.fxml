            <Slider fx:id="slider" layoutX="17.0" layoutY="62.0" max="90.0" min="10.0" prefHeight="14.0" prefWidth="493.0" />
            <Button layoutX="28.0" layoutY="148.0" mnemonicParsing="false" onAction="#startFirst" text="start1" />
            <Button fx:id="decBtn" layoutX="28.0" layoutY="205.0" mnemonicParsing="false" onAction="#stopFirst" text="stop1" />
            <Button layoutX="410.0" layoutY="148.0" mnemonicParsing="false" onAction="#startSecond" text="start2" />
            <Button fx:id="incBtn" layoutX="410.0" layoutY="218.0" mnemonicParsing="false" onAction="#stopSecond" text="stop2" />
